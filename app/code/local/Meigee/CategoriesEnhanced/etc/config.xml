<?xml version="1.0"?>
<config>
	<modules>
		<Meigee_CategoriesEnhanced>
			<version>1.1</version>
			<depends>
			<!-- no dependencies -->
			</depends>
		</Meigee_CategoriesEnhanced>
	</modules>
	<global>
		<models>
			<Meigee_CategoriesEnhanced><class>Meigee_CategoriesEnhanced_Model</class></Meigee_CategoriesEnhanced>
		</models>
		<resources>
		    <categoriesenhanced_setup>
		      <setup>
		        <module>Meigee_CategoriesEnhanced</module>
		        <class>Meigee_CategoriesEnhanced_Model_Resource_Eav_Mysql4_Setup</class>
		      </setup>
		      <connection>
		        <use>core_setup</use>
		      </connection>
		    </categoriesenhanced_setup>
		    <categoriesenhanced_write>
		      <connection>
		        <use>core_write</use>
		      </connection>
		    </categoriesenhanced_write>
		    <categoriesenhanced_read>
		      <connection>
		        <use>core_read</use>
		      </connection>
		    </categoriesenhanced_read>
		</resources>
		<events />
		<helpers>
			<Meigee_CategoriesEnhanced><class>Meigee_CategoriesEnhanced_Helper</class></Meigee_CategoriesEnhanced>
		</helpers>
		<extraconfig />
		<blocks>
			<page>
				<rewrite>
					<html_topmenu>Meigee_CategoriesEnhanced_Block_Html_Topmenu</html_topmenu>
				</rewrite>
			</page>
		</blocks>
	</global>
	<default>
		<meigee_categoriesenhanced>
			<options>
				<status>1</status>
				<column_count>3</column_count>
				<double_line_status>0</double_line_status>
				<double_line_item_width>85</double_line_item_width>
				<vertical_menu_status>0</vertical_menu_status>
				<vertical_menu_id></vertical_menu_id>
				<vertical_menu_type>default-open</vertical_menu_type>
				<vertical_menu_skin>skin-1</vertical_menu_skin>
			</options>
		</meigee_categoriesenhanced>
	</default>
</config>
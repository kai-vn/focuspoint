}

/* Search */
.header .search-mini-form .form-search input {
    display: inline-block;
    border-radius: 0;
    border-color: #555555;
	border: 1px solid #555555;
    background-color: transparent;
    box-shadow: none;
    color: rgba(0,0,0,.4);
    font-size: 13px;
    vertical-align: middle;
    height: auto;
	padding: 6px 12px;
    width: 209px;
}

.header .search-mini-form .form-search button {
	height: auto;
	background:#555555;
	margin-left:-3px;
	width: auto;
	padding: 6px 12px;
	border: 1px solid #555555;
	
}

.header .search-mini-form .form-search .btn {
	text-transform:none;
	background-color: #555555;
    border-color: #555555;
    color: #fff;
    padding: 6px 12px;
	border-radius: 0;
    
}

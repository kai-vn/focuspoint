@import url('https://fonts.googleapis.com/css?family=Lato');
body { font-family: 'Lato', sans-serif;}
.wide-header .header-breadcrumbs-wrapper,
.wide-content .content-wrapper {background-color: transparent;}
/* Common */
.input-captcha, input.form-control, textarea.form-control, select.form-control {
    -webkit-box-shadow: none;
    box-shadow: none;
    border-radius: 0px;
    background-color: #fff;
	border: 1px solid #000000;
    color: rgba(0,0,0,0.4);
    border-color: #000000;
    border-width: 1px;
    padding: 6px 12px;
    font-size: 13px;
}
.page-title {
    border-bottom: 0px solid rgba(0, 0, 0, 0.1);
    
}
.block-title {
    margin: 0 0 15px;
    font-family: 'Lato', sans-serif;
    color: #222;
    font-weight: 300;
    padding: 0 0 14px;
    font-size: 14px;
	font-weight:700;
    position: relative;
    text-transform: uppercase;
    letter-spacing: 0;
    border-bottom: solid 0px rgba(0, 0, 0, 0.1);
}

/* Breadcrumbs*/
.breadcrumbs-wrapper.type-3 .page-title + .breadcrumb {
    float: none;
    display: block;
	text-align:left;
	margin-bottom: 15px;
}
.breadcrumbs-wrapper.type-3 .breadcrumb {
    color: #000000;
	font-family: 'Lato', sans-serif;
	text-transform:uppercase;
}

.catalog-category-view .breadcrumbs-wrapper.type-3 .page-title + .breadcrumb { text-align:center; }
.breadcrumbs-wrapper .breadcrumb li:last-of-type:before {
    display:none;
}
.breadcrumbs-wrapper .meigee-arrow-right:before {
    content: '\0044';
    font-size: 10px;
    color: #000000;
    padding: 0px 1px 0px 5px;
}
.breadcrumbs-wrapper .breadcrumb > li + li:before {
    display:none;
}
/* Header */



#header.header-4 .middle-block .container {
    padding-top: 0px;
    padding-bottom: 0px;
}

.wide-header #header .bottom-usp-block {
    background-color: #000000;
	margin-bottom: 20px;
}
.wide-header #header .bottom-usp-block .container {
   
	text-align:center;
}


/* Menu */
#header .bottom-block .bottom-block-inner {
    background-color: #ffffff;
    border-radius: 4px;
    margin-bottom: 0px;
    position: relative;
}
ul.topmenu {
    border-width: 0;
    padding: 0;
    margin: 0;
    font-family: 'Lato', sans-serif;
}
body .navbar-default {
    background: none;
    border: none;
    background-image: none;
    box-shadow: none;
    position: static;
    min-height: 1px;
    margin: 0;
    float: left;
    text-align: center;
    margin: 0 20%;
}
ul.topmenu li.level0.parent a.level-top:after {
    content: "";
    display: none;
    width: 0;
    height: 0;
    border-top: 4px dashed;
    border-right: 4px solid transparent;
    border-left: 4px solid transparent;
    border-top-color: rgba(0,0,0,.3);
    vertical-align: middle;
    margin-left: 3px;
    position: relative;
    top: -1px;
}
ul.topmenu li.level0 a.level-top {
    position: relative;
    font-family: 'Lato', sans-serif;
    font-size: 14px;
    font-weight: 600;
    line-height: 1.58;
    color: #222;
    padding: 16px 22px;
    z-index: 999;
    background: none;
    text-transform: uppercase;
    transition: none;
    -moz-transition: none;
    -webkit-transition: none;
}
#header.header-4 .bottom-block .bottom-block-inner li.first a.level-top {
    border-radius: 0px; 
    padding-left: 20px;
}

